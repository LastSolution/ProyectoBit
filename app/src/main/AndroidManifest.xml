        android:theme="@style/Theme.Tastit">
        <activity android:name=".NombresClientes"></activity>
        <activity android:name=".MesaBienvenida" />
        <activity android:name=".Activity_Mesa" />
        <activity android:name=".PlatosActivity" />
        <activity android:name=".abm_empresa" />
        <activity android:name=".UsuariosActivity" />
        <activity android:name=".EmpresaActivity" />
        <activity
            android:name=".AbmActivity"
            android:label="@string/title_activity_abm" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>